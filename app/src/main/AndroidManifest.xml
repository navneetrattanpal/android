<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pawan.studentteacherapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Connectiontoparse"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Wallpaper" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".HomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name=".RegisterPage"
            android:label="@string/title_activity_register_page" >
        </activity>
        <activity
            android:name=".StudentPage"
            android:label="@string/title_activity_student_page" >
        </activity>
        <activity
            android:name=".TeacherPage"
            android:label="@string/title_activity_teacher_page" >
        </activity>
        <activity
            android:name=".TeacherAddClass"
            android:label="@string/title_activity_teacher_add_class" >
        </activity>
        <activity
            android:name=".BlogArea"
            android:label="@string/title_activity_blog_area" >
        </activity>
    </application>

</manifest>
